{
  "name": "@domonic18/claude-code-ui",
  "version": "1.13.6",
  "description": "A web-based UI for Claude Code CLI",
  "type": "module",
  "main": "backend/index.js",
  "bin": {
    "claude-code-ui": "backend/cli.js",
    "cloudcli": "backend/cli.js"
  },
  "files": [
    "backend/",
    "shared/",
    "dist/",
    "README.md"
  ],
  "homepage": "https://claudecodeui.siteboon.ai",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domonic18/ai-claude-code-ui.git"
  },
  "bugs": {
    "url": "https://github.com/domonic18/ai-claude-code-ui/issues"
  },
  "scripts": {
    "dev": "concurrently --kill-others \"npm run server\" \"npm run client\"",
    "server": "node backend/index.js",
    "client": "vite --host",
    "build": "vite build",
    "preview": "vite preview",
    "start": "npm run build && npm run server",
    "release": "./scripts/release.sh",
    "test": "npm run test:imports && npm run test:database && npm run test:user-settings-mcp && npm run test:api-endpoints && npm run test:container && npm run test:docker && npm run test:fileops && npm run test:sdk",
    "test:imports": "node backend/tests/unit/module-imports.test.js",
    "test:database": "node backend/tests/unit/database.test.js",
    "test:user-settings-mcp": "node backend/tests/unit/user-settings-mcp.test.js",
    "test:api-endpoints": "node backend/tests/integration/api-endpoints.test.js",
    "test:container": "node backend/tests/integration/container.test.js",
    "test:docker": "node backend/tests/integration/docker-container.test.js",
    "test:fileops": "node backend/tests/integration/file-operations.test.js",
    "test:sdk": "node backend/tests/integration/claude-sdk-invocation.test.js"
  },
  "keywords": [
    "claude coode",
    "ai",
    "anthropic",
    "ui",
    "mobile"
  ],
  "author": "domonic18",
  "license": "MIT",
